{
  "name": "albertwill/yii2-rabbitmq",
  "description": "Maintained fork of mikemadisonweb/yii2-rabbitmq that fixes duplicate multi-connection registration issues and improves worker process robustness.",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","rabbitmq","amqp","message queue","distributed systems"],
  "license": "MIT",
  "authors": [
    {
      "name": "AlbertWill",
      "email": "1073658683@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "^2.0.43",
    "php-amqplib/php-amqplib": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "php-coveralls/php-coveralls": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "mikemadisonweb\\rabbitmq\\": ""
    }
  },
  "extra": {
    "bootstrap": "mikemadisonweb\\rabbitmq\\DependencyInjection"
  }
}
